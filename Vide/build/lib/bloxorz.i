void itoa(uint16_t number, char* text)
{
 uint16_t muls[] = { 100, 10, 1 };
 if (number > 999) number = 999;
 for (uint8_t i = 0; i < 3; i++) {
  uint8_t d = 0;
  while (number >= muls[i]) {
   d++;
   number -= muls[i];
  }
  text[i] = d + '0';
 }
}

